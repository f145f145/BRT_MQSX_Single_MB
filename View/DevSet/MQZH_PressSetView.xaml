                            <TextBox Text="{Binding PublicData.Dev.PressSet_SMDJ_WD_False[0], Mode=TwoWay}"  Grid.Row="0" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding PublicData.Dev.PressSet_SMDJ_WD_False[1], Mode=TwoWay}"  Grid.Row="1" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding PublicData.Dev.PressSet_SMDJ_WD_False[2], Mode=TwoWay}"  Grid.Row="2" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding PublicData.Dev.PressSet_SMDJ_WD_False[3], Mode=TwoWay}"  Grid.Row="3" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
