                            <Label Content="定级负压预加压：" Grid.Row="4" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="定级负压检测第1级：" Grid.Row="5" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="定级负压检测第2级：" Grid.Row="6" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="定级负压检测第3级："  Grid.Row="7" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <TextBox Text="{Binding PublicData.Dev.PressSet_QMDJ_Std[0][0], Mode=TwoWay}" Grid.Row="0" Grid.Column ="1" Style="{StaticResource TextBox1}" IsEnabled="False"/>
                            <TextBox Text="{Binding PublicData.Dev.PressSet_QMDJ_Std[1][0], Mode=TwoWay}" Grid.Row="1" Grid.Column ="1" Style="{StaticResource TextBox1}" IsEnabled="False"/>
                            <TextBox Text="{Binding PublicData.Dev.PressSet_QMDJ_Std[1][1], Mode=TwoWay}" Grid.Row="2" Grid.Column ="1" Style="{StaticResource TextBox1}" IsEnabled="False"/>
