                            <CheckBox Content="启用3号" IsChecked ="{Binding Dev.AIList[0].CalPoints[2].IsUse,Mode =TwoWay}" Grid.Row="2" Grid.Column="3" />
                            <CheckBox Content="启用4号" IsChecked ="{Binding Dev.AIList[0].CalPoints[3].IsUse,Mode =TwoWay}" Grid.Row="2" Grid.Column="4" />
                            <CheckBox Content="启用5号" IsChecked ="{Binding Dev.AIList[0].CalPoints[4].IsUse,Mode =TwoWay}" Grid.Row="2" Grid.Column="5" />
                            <CheckBox Content="启用6号" IsChecked ="{Binding Dev.AIList[0].CalPoints[5].IsUse,Mode =TwoWay}" Grid.Row="2" Grid.Column="6" />
                            <CheckBox Content="启用7号" IsChecked ="{Binding Dev.AIList[0].CalPoints[6].IsUse,Mode =TwoWay}" Grid.Row="2" Grid.Column="7" />
