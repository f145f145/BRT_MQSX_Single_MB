                            <Label Content="量程下限" Grid.Row ="0" Grid.Column="2" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.AIList[0].SingalLowerRange,Mode =TwoWay}" Grid.Row ="0" Grid.Column="3" Style="{StaticResource TextBox1}" IsEnabled =" False"/>
                            <Label Content="量程上限" Grid.Row ="0" Grid.Column="4" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
