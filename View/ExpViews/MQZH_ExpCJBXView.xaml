<Window x:Class="MQZHWL.View.ExpViews.MQZH_ExpCJBXView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        xmlns:local="clr-namespace:MQZHWL.View.ExpViews"
        xmlns:converterPath="clr-namespace:MQZHWL.UICtl.Converter"
        mc:Ignorable="d"
        Title="建筑幕墙四性能检测—层间变形"
        Width="1600" Height="850" MinWidth="1280" MinHeight="850"
        WindowStartupLocation="CenterScreen" Background="White" ShowInTaskbar="True"
        Closing="Window_Closing" WindowState="Maximized"
        DataContext ="{Binding Source ={StaticResource Locator },Path =Main_ViewModel}">

    <Window.Resources>
        <!--紧急按钮模板-->
        <ControlTemplate TargetType="Button" x:Key="EmStpButton">
            <Border Background="Yellow" Name="back" BorderBrush="Red" BorderThickness="1"
                    CornerRadius="3" Padding="5"
                    TextBlock.Foreground="Red" TextBlock.FontSize="18" TextBlock.FontWeight="Bold"
                    Width="100" Height ="50">
                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="back"  Property="Background" Value="Aqua"/>
                    <Setter Property="TextElement.Foreground" TargetName="back" Value="Red"/>
                </Trigger>
                <Trigger Property="IsPressed" Value="true">
                    <Setter TargetName="back" Property="Background" Value="BlueViolet"/>
                    <Setter Property="TextElement.Foreground" TargetName="back" Value="White"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <!--普通按钮模板-->
        <ControlTemplate TargetType="Button" x:Key="Button1">
            <Border Background="LightGray" Name="back" BorderBrush="Black" BorderThickness="0.8"
                    CornerRadius="3" Padding="5"
                    TextBlock.Foreground="Black" TextBlock.FontSize="18" Width="100" Height ="50">
                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="back"  Property="Background" Value="snow"/>
                </Trigger>
                <Trigger Property="IsPressed" Value="true">
                    <Setter TargetName="back" Property="Background" Value="CadetBlue"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <!--默认CheckBox样式-->
        <Style TargetType="CheckBox">
            <Style.Setters>
                <Setter Property="HorizontalAlignment" Value="Left"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontSize" Value="20"/>
                <Setter Property="FontWeight" Value="Bold"/>
                <Setter Property="Tag">
                    <Setter.Value>
                        <SolidColorBrush Color="Black"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="CheckBox">
                            <StackPanel Orientation="Horizontal">
                                <Border x:Name="bd" BorderBrush="Black" 
                                        BorderThickness="2" Margin="5,2">
                                    <Path x:Name="cp" Width="15" Height="15"  
                                          Stroke="Blue"  
                                          StrokeThickness="3"/>
                                </Border>
                                <ContentPresenter/>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsChecked" Value="True">
                                    <Setter TargetName="cp" Property="Data"  
                                            Value="M 0,6 L 6,12 12,0"/>
                                    <Setter TargetName="bd" 
                                            Property="BorderBrush">
                                        <Setter.Value>
                                            <SolidColorBrush Color="Blue"/>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style.Setters>
        </Style>
        <!--Label模板1，参数名-->
        <Style x:Key="Label1" TargetType="Label">
            <Setter Property="FontSize" Value="20" />
            <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板2，参数值-->
        <Style x:Key="Label2" TargetType="Label">
            <Setter Property="Height" Value="40"/>
            <Setter Property="Width" Value="100"/>
            <Setter Property="FontSize" Value="24"/>
            <Setter Property="FontFamily" Value="{DynamicResource DigifaceWide}"/>
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板3，参数单位-->
        <Style x:Key="Label3" TargetType="Label">
            <Setter Property="FontSize" Value="18"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板4，试验信息-->
        <Style x:Key="Label4" TargetType="Label">
            <Setter Property="Height" Value="35"/>
            <Setter Property="Width" Value="280"/>
            <Setter Property="FontSize" Value="18"/>
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板5,完成状态-->
        <Style x:Key="Label5" TargetType="Label">
            <Setter Property="FontSize" Value="20"></Setter>
            <Setter Property="Height" Value="30"></Setter>
            <Setter Property="Width" Value="30"></Setter>
            <Setter Property="FontWeight" Value="Bold"></Setter>
            <Setter Property="HorizontalAlignment" Value="Center"></Setter>
            <Setter Property="VerticalAlignment" Value="Center"></Setter>
            <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
            <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
            <Setter Property="Margin" Value="5"></Setter>
        </Style>
        <!--圆圈模版2，无色-->
        <Style x:Key="Ellipse2" TargetType="Ellipse">
            <Setter Property="Height" Value="20"/>
            <Setter Property="Width" Value="20"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5"/>
            <Setter Property="Stroke" Value="Black"/>
            <Setter Property="Fill" Value="DarkGray"/>
        </Style>
        <!--值转换器-->
        <converterPath:BoolToExpTypeConverter x:Key="ExpTypeConverter"/>
        <converterPath:BoolToExpCompleteConverter x:Key="ExpCompleteConverter"/>
        <converterPath:BoolToDevStatusWZConverter x:Key="DevStatusWzConverter"/>
        <converterPath:BoolToCollorGConverter x:Key="BGConverter"/>
    </Window.Resources>

    <Grid x:Name="GridMain">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.3*" MinWidth="550"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition  Width="*" MinWidth="550"/>
        </Grid.ColumnDefinitions>

        <!--=====菜单栏===================================================================================================-->
        <Menu Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" VerticalContentAlignment="Center" Height ="40">
            <MenuItem Header="试验操作"  FontSize="16"  Height ="40">
                <MenuItem Header="终止试验"  FontSize="16" Command="{Binding CJBXCommand}" CommandParameter="1207" Height ="40" VerticalContentAlignment="Center"/>
                <MenuItem Header="修改试验请退出试验后在主窗口操作" IsEnabled="False" FontSize="16" Height ="40"/>
            </MenuItem>
            <MenuItem Header="试验数据"  FontSize="16" Height ="40">
                <MenuItem Header="定级检测损坏情况" Command="{Binding CJBXCommand}" CommandParameter="7116" FontSize="16" Height ="40" VerticalContentAlignment="Center"/>
                <Separator Background="Black"  ></Separator>
                <MenuItem Header="工程检测损坏情况" Command="{Binding CJBXCommand}" CommandParameter="7117" FontSize="16" Height ="40" VerticalContentAlignment="Center"/>
                <Separator Background="Black"  ></Separator>
                <MenuItem Header="当前试验数据" Command="{Binding CJBXCommand}" CommandParameter="7104" FontSize="16" Height ="60" VerticalContentAlignment="Center"/>
            </MenuItem>
            <MenuItem Header="位移零点"  FontSize="16" Height ="40">
                <MenuItem Header="设置X轴位移零点" Command="{Binding CJBXCommand}" CommandParameter="1630" FontSize="16" Height ="40"/>
                <MenuItem Header="设置Y轴位移零点" Command="{Binding CJBXCommand}" CommandParameter="1631" FontSize="16" Height ="40"/>
                <MenuItem Header="设置Z轴位移零点" Command="{Binding CJBXCommand}" CommandParameter="1632" FontSize="16" Height ="40"/>
                <MenuItem Header="设置XYZ三轴位移零点" Command="{Binding CJBXCommand}" CommandParameter="1633" FontSize="16" Height ="40"/>
            </MenuItem>
            <MenuItem Header="退出检测"  FontSize="16" Height ="40">
                <MenuItem Header="退出层间变形检测" Command="{Binding CJBXCommand}" CommandParameter="7211" FontSize="16" Height ="40"/>
            </MenuItem>
        </Menu>

        <!--=====左半屏，参数状态显示、实时曲线===========================================================================-->
        <Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment ="Stretch"  VerticalAlignment="stretch">
            <Grid.RowDefinitions>
                <RowDefinition  Height="*" MinHeight="200"/>
                <RowDefinition Height="auto"/>
                <RowDefinition  Height="3*" MinHeight="300"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <!--左上屏，参数、状态显示区块-->
            <Border Grid.Row="0" Margin="5" HorizontalAlignment="stretch" VerticalAlignment="stretch" CornerRadius="5" Background="WhiteSmoke">
                <Grid >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="Y轴位移左：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="0"/>
                    <Label Content="Y轴位移中：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="0"/>
                    <Label Content="Y轴位移右：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="0"/>
                    <Label Content="{Binding Dev.WYNOList[1]}" ToolTip ="Y轴左位移尺编号" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="1"/>
                    <Label Content="{Binding Dev.WYNOList[2]}" ToolTip ="Y轴中位移尺编号" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="1"/>
                    <Label Content="{Binding Dev.WYNOList[3]}" ToolTip ="Y轴右位移尺编号" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="1"/>
                    <Label Content="{Binding Dev.WYValueY[0]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Y轴左位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="2"/>
                    <Label Content="{Binding Dev.WYValueY[1]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Y轴中位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="2"/>
                    <Label Content="{Binding Dev.WYValueY[2]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Y轴右位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="2"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="0" Grid.Column ="3"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="1" Grid.Column ="3"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="2" Grid.Column ="3"/>

                    <Label Content="Z轴位移左：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="4"/>
                    <Label Content="Z轴位移中：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="4"/>
                    <Label Content="Z轴位移右：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="4"/>
                    <Label Content="{Binding Dev.WYNOList[4]}" ToolTip ="Z轴左位移尺编号" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="5"/>
                    <Label Content="{Binding Dev.WYNOList[5]}" ToolTip ="Z轴中位移尺编号" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="5"/>
                    <Label Content="{Binding Dev.WYNOList[6]}" ToolTip ="Z轴右位移尺编号" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="5"/>
                    <Label Content="{Binding Dev.WYValueZ[0]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Z轴左位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="6"/>
                    <Label Content="{Binding Dev.WYValueZ[1]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Z轴中位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="6"/>
                    <Label Content="{Binding Dev.WYValueZ[2]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Z轴右位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="6"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="0" Grid.Column ="7"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="1" Grid.Column ="7"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="2" Grid.Column ="7"/>

                    <Label Content="X  轴  位  移：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="8"/>
                    <Label Content="Y轴平均位移：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="8"/>
                    <Label Content="Z轴平均位移：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="8"/>
                    <Label Content="{Binding Dev.WYNOList[0]}" ToolTip ="X轴位移尺编号" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="9"/>
                    <Label Content="{Binding Dev.WYValueX}" ContentStringFormat="{}{0:0.00}" ToolTip ="X轴位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="10"/>
                    <Label Content="{Binding Dev.WYValueY[3]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Y轴平均位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="10"/>
                    <Label Content="{Binding Dev.WYValueZ[3]}" ContentStringFormat="{}{0:0.00}" ToolTip ="Z轴平均位移值（调零后）" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="10"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="0" Grid.Column ="11"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="1" Grid.Column ="11"/>
                    <Label Content="mm" Style="{StaticResource Label3}" Grid.Row="2" Grid.Column ="11"/>
                </Grid>
            </Border>

            <!--横向分割线-->
            <GridSplitter Grid.Row="1"  Height="3" VerticalAlignment="Center" HorizontalAlignment="stretch"/>

            <!--左下屏，场景及曲线图区块-->
            <TabControl HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="10" Grid.Row="2"  >
                <TabItem Header="【实时曲线】" FontSize="15" Padding="5" IsEnabled="True" >
                    <d3:ChartPlotter x:Name="MQWL_ChartPlotter"  Margin="5,5,5,5" >
                        <d3:VerticalAxisTitle x:Name="AxisY_Default_Title" Content="检测值"/>
                        <d3:HorizontalAxisTitle x:Name="AxisX_Title" Content="时间"/>
                        <d3:AxisGrid Visibility ="Visible "></d3:AxisGrid>
                        <!--添加曲线-->
                        <d3:LineGraph Name="LineCJBX01" StrokeThickness="0.8" DataSource="{Binding PlotLines[8].LineDataSource}" Stroke ="Red"></d3:LineGraph>
                        <d3:LineGraph Name="LineCJBX02" StrokeThickness="0.8" DataSource="{Binding PlotLines[12].LineDataSource}" Stroke ="DarkBlue"></d3:LineGraph>
                        <d3:LineGraph Name="LineCJBX03" StrokeThickness="0.8" DataSource="{Binding PlotLines[16].LineDataSource}" Stroke ="DarkGreen"></d3:LineGraph>
                        <d3:CursorCoordinateGraph  Name="Cross" AutoHide="True" />
                    </d3:ChartPlotter>
                </TabItem>
            </TabControl>

            <!--左屏底端，提醒、警告信息栏-->
            <Label Content="{Binding Dev.ErrorInfo}"  FontSize="20" FontWeight="Bold" Foreground="#FFFF7400"
                   Grid.Row="3" Margin="5"  HorizontalAlignment="Center" VerticalAlignment="Center" />
        </Grid>

        <!--=========================================纵向分割线==========================================================-->
        <GridSplitter Grid.Row="1" Grid.Column="1" Width="3" HorizontalAlignment="Center" VerticalAlignment="stretch"/>

        <!--=========================================右半屏，试验基本信息及控制==========================================-->
        <Grid Grid.Row="1" Grid.Column="2" HorizontalAlignment ="Stretch "  VerticalAlignment="stretch">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" MinHeight="50"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="300"/>
                <ColumnDefinition Width="*" MinWidth="400"/>
            </Grid.ColumnDefinitions>
            <!--右半屏标题-->
            <Label Content="试验信息及控制（层间变形性能检测）" FontSize="24" FontWeight="Bold" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,5,5,5" HorizontalAlignment="Center" VerticalAlignment="Center"/>

            <!--试验信息-->
            <Border Grid.Row="1" Grid.Column="0" Margin="5" HorizontalAlignment="stretch" VerticalAlignment="stretch" CornerRadius="5" Background="WhiteSmoke">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="auto" MinHeight="50"/>
                    </Grid.RowDefinitions>
                    <StackPanel Grid.Row="0" HorizontalAlignment="stretch" VerticalAlignment="Top">
                        <Label Content="试验编号" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <Label Content="{Binding ExpDQ.ExpSettingParam.ExpNO}" Style="{StaticResource Label4}"/>
                        <Label Content="检测模式" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <Label Content="{Binding ExpDQ.Exp_CJBX.IsGC, Converter={StaticResource ExpTypeConverter}}" Style="{StaticResource Label4}"/>
                        <Label Content="当前项目" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <Label Content="{Binding ExpDQ.Exp_CJBX.ExpXM_CJBXStage_DQ.StageName}" Style="{StaticResource Label4}"/>
                        <Label Content="当前步骤" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <Label Content="{Binding ExpDQ.Exp_CJBX.ExpXM_CJBXStep_DQ.StepName,Mode=OneWay}" Style="{StaticResource Label4}"/>
                        <Label Content="试样定位目标" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <Label Content="{Binding ExpDQ.Exp_CJBX.ExpXM_CJBXStep_DQ.AimPosition,Mode=OneWay}" Style="{StaticResource Label4}"/>
                        <Label Content="设备定位目标" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <TextBlock  Text="{Binding ExpDQ.Exp_CJBX.ExpXM_CJBXStep_DQ.AimPositionSB,Mode=OneWay}" Height="30" FontSize="16"/>
                        <Label Content="设备状态" FontSize="20" Margin="0,10,0,0" HorizontalAlignment="Left"/>
                        <Label Content="{Binding Dev.DeviceRunMode, Converter={StaticResource DevStatusWzConverter}}" Style="{StaticResource Label4}"/>
                    </StackPanel>
                    <Button Content="紧急停机" Command="{Binding CJBXCommand}" CommandParameter="119" Template="{StaticResource EmStpButton}" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin ="5"/>
                </Grid>
            </Border>

            <!--试验阶段状态及选择-->
            <Border Grid.Row="1" Grid.Column="1" Margin="5" HorizontalAlignment="stretch" VerticalAlignment="stretch" CornerRadius="5" Background="WhiteSmoke">
                <TabControl HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="0"  >
                    <TabItem Header="【定级X轴】" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" FontSize="15" Padding="5">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="80"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2*" MinWidth ="220"/>
                                <ColumnDefinition Width="*" MinWidth ="50"/>
                            </Grid.ColumnDefinitions>
                            <Label Content="阶段选择" Grid.Row="0" Grid.Column="0" Style="{StaticResource Label1}" VerticalAlignment="Center" HorizontalAlignment="Center" />
                            <Label Content="完成&#x000A;情况" Grid.Row="0" Grid.Column="1" Style="{StaticResource Label1}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin ="5"/>
                            <CheckBox Content="定级X轴 预加载" IsChecked ="{Binding ExpDQ.Exp_CJBX.BeenCheckedDJX[0],Mode =TwoWay}" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" Grid.Row="1" Grid.Column="0" />
                            <CheckBox Content="定级X轴 1级检测加载" IsChecked ="{Binding ExpDQ.Exp_CJBX.BeenCheckedDJX[1],Mode =TwoWay}" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" Grid.Row="2" Grid.Column="0"/>
                            <CheckBox Content="定级X轴 2级检测加载" IsChecked ="{Binding ExpDQ.Exp_CJBX.BeenCheckedDJX[2],Mode =TwoWay}" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" Grid.Row="3" Grid.Column="0" />
                            <CheckBox Content="定级X轴 3级检测加载" IsChecked ="{Binding ExpDQ.Exp_CJBX.BeenCheckedDJX[3],Mode =TwoWay}" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" Grid.Row="4" Grid.Column="0"/>
                            <CheckBox Content="定级X轴 4级检测加载" IsChecked ="{Binding ExpDQ.Exp_CJBX.BeenCheckedDJX[4],Mode =TwoWay}" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" Grid.Row="5" Grid.Column="0" />
                            <CheckBox Content="定级X轴 5级检测加载" IsChecked ="{Binding ExpDQ.Exp_CJBX.BeenCheckedDJX[5],Mode =TwoWay}" IsEnabled ="{Binding ExpDQ.Exp_CJBX.IsDJcanbeChecked}" Grid.Row="6" Grid.Column="0"/>
                            <Label Content="{Binding ExpDQ.Exp_CJBX.StageList_DJX[0].CompleteStatus, Converter={StaticResource ExpCompleteConverter}}" Grid.Row="1" Grid.Column="1" Style="{StaticResource Label5}"/>
                            <Label Content="{Binding ExpDQ.Exp_CJBX.StageList_DJX[1].CompleteStatus, Converter={StaticResource ExpCompleteConverter}}" Grid.Row="2" Grid.Column="1" Style="{StaticResource Label5}"/>
                            <Label Content="{Binding ExpDQ.Exp_CJBX.StageList_DJX[2].CompleteStatus, Converter={StaticResource ExpCompleteConverter}}" Grid.Row="3" Grid.Column="1" Style="{StaticResource Label5}"/>
                            <Label Content="{Binding ExpDQ.Exp_CJBX.StageList_DJX[3].CompleteStatus, Converter={StaticResource ExpCompleteConverter}}" Grid.Row="4" Grid.Column="1" Style="{StaticResource Label5}"/>
                            <Label Content="{Binding ExpDQ.Exp_CJBX.StageList_DJX[4].CompleteStatus, Converter={StaticResource ExpCompleteConverter}}" Grid.Row="5" Grid.Column="1" Style="{StaticResource Label5}"/>
                            <Label Content="{Binding ExpDQ.Exp_CJBX.StageList_DJX[5].CompleteStatus, Converter={StaticResource ExpCompleteConverter}}" Grid.Row="6" Grid.Column="1" Style="{StaticResource Label5}"/>
                            <!--开始、终止测试控制按钮栏-->
                            <Button Content="开始测试" Command="{Binding CJBXCommand}" CommandParameter="1107" Template="{StaticResource Button1}" Grid.Row="9" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin ="30,5,5,5"/>
                            <Button Content="停止测试" Command="{Binding CJBXCommand}" CommandParameter="1207" Template="{StaticResource Button1}" Grid.Row="9" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin ="5,5,30,5"/>
                        </Grid>
                    </TabItem>

