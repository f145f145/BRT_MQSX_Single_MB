<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MQDFJ_MB.View"
                    xmlns:converterPath="clr-namespace:MQDFJ_MB.UICtl.Converter">
    
    <!--==========普通按钮==========-->
    <Style TargetType="{x:Type Button}">
        <Setter Property="Width" Value="160"/>
        <Setter Property="Height" Value="50"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="Background" Value="LightGray"/>
        <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="back" 
                            Background="LightGray"
                            BorderBrush="{TemplateBinding Control.BorderBrush}"
                            Width="{TemplateBinding ContentControl.Width}"
                            Height="{TemplateBinding ContentControl.Height}"
                            BorderThickness="0.8" CornerRadius="3" >
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="back" Property="Background" Value="snow"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter TargetName="back" Property="Background" Value="CadetBlue"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--普通按钮模板2-->
    <ControlTemplate TargetType="{x:Type Button}" x:Key="Button2">
        <Border Background="LightGray" x:Name="back" BorderBrush="Black" BorderThickness="0.8"
                    CornerRadius="3" Padding="0" HorizontalAlignment="Center"
                    TextBlock.Foreground="Black" TextBlock.FontSize="16" Width="90" Height ="30" Margin="0">
            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter TargetName="back" Property="Background" Value="snow"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="true">
                <Setter TargetName="back" Property="Background" Value="CadetBlue"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    
    <!--==========菜单按钮==========-->
    <Style TargetType="{x:Type Button}" x:Key="MenuButton">
        <Setter Property="Width" Value="140"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="Background" Value="LightGray"/>
        <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="back" 
                            BorderBrush="{TemplateBinding Control.BorderBrush}"
                            Width="{TemplateBinding ContentControl.Width}"
                            Height="{TemplateBinding ContentControl.Height}"
                            BorderThickness="0.8" CornerRadius="3" >
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="back" Property="Background" Value="DeepSkyBlue"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter TargetName="back" Property="Background" Value="CadetBlue"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--==========数字量开关按钮==========-->
    <Style TargetType="{x:Type Button}" x:Key="IOButton">
        <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Height" Value="90"/>
        <Setter Property="Width" Value="155"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="back" 
                            Background="{TemplateBinding ContentControl.Background}"
                            BorderBrush="{TemplateBinding Control.BorderBrush}"
                            Width="{TemplateBinding ContentControl.Width}"
                            Height="{TemplateBinding ContentControl.Height}"
                            BorderThickness="0.8" CornerRadius="5">
                        <ContentPresenter HorizontalAlignment="{TemplateBinding ContentControl.HorizontalAlignment}" VerticalAlignment="{TemplateBinding ContentControl.VerticalAlignment}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="back" Property="Background" Value="DeepSkyBlue"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter TargetName="back" Property="Background" Value="CadetBlue"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--==========紧急停机按钮模板==========-->
    <Style TargetType="{x:Type Button}" x:Key="EmStpButton">
        <Setter Property="Width" Value="140"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="Foreground" Value="Red"/>
        <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="back" 
                            BorderBrush="Red"
                            Background="Gold" 
                            Width="{TemplateBinding ContentControl.Width}"
                            Height="{TemplateBinding ContentControl.Height}"
                            BorderThickness="0.8" CornerRadius="3" >
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="back" Property="Background" Value="Aqua"/>
                            <Setter Property="Foreground" Value="Red"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter TargetName="back" Property="Background" Value="White"/>
                            <Setter Property="Foreground" Value="DarkOrchid"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

