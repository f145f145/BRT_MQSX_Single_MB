                            <TextBox Text="{Binding Dev.AIList[0].CalPoints[10].StdValue ,Mode =TwoWay}" Grid.Row ="3" Grid.Column="11" Style="{StaticResource TextBox1}"  IsEnabled ="{Binding Dev.AIList[0].CalPoints[10].IsUse}"/>

