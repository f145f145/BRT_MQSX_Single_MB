                            <ComboBox ItemsSource="{Binding Dev.InfTypeList}" SelectedItem ="{Binding Dev .Mod_04AD1.Channels[1].InfType, Mode=TwoWay}" Grid.Row="2" Grid.Column ="1" Style="{StaticResource ComboBox1}" IsEnabled ="False"/>
