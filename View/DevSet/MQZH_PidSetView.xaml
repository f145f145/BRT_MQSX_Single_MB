                    <Border BorderBrush="Black" Grid.Row="0" Grid.Column ="0" BorderThickness="1" CornerRadius="5"  HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="5">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="140"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Label Content="被控下限：" Grid.Row="0" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="被控上限：" Grid.Row="1" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="比例系数kp：" Grid.Row="2" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="积分系数ki："  Grid.Row="3" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="微分系数kd："  Grid.Row="4" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <Label Content="积分限幅：" Grid.Row="5" Grid.Column ="0" Style="{StaticResource Label1}"/>
                            <TextBox Text="0 m³/h"  Grid.Row="0" Grid.Column ="1" IsEnabled="False" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding Dev.AIList[18].SingalUpperRange, Mode=TwoWay}"  Grid.Row="1" Grid.Column ="1" IsEnabled="False" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding Dev.PID51.Kp, Mode=TwoWay}"  Grid.Row="2" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding Dev.PID51.Ki, Mode=TwoWay}"   Grid.Row="3" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding Dev.PID51.Kd, Mode=TwoWay}"   Grid.Row="4" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                            <TextBox Text="{Binding Dev.PID51.U_IMax_Limit, Mode=TwoWay}"  Grid.Row="5" Grid.Column ="1" Style="{StaticResource TextBox1}"/>
                        </Grid>
                    </Border>
                  </Grid>
            </TabItem>

        </TabControl>
        <Button Content="保存设置" Command="{Binding DevSetCommand}" CommandParameter="6" Grid.Row="1" Grid.Column ="2" Template="{StaticResource Button1}" HorizontalAlignment="Left" VerticalAlignment="Center" IsDefault="True"/>
        <Button Content="取消修改" Command="{Binding DevSetCommand}" CommandParameter="99"  Grid.Row="1" Grid.Column ="2" Template="{StaticResource Button1}" HorizontalAlignment="Right" VerticalAlignment="Center" IsCancel="True"/>
        <Label Content="此页内数据请勿擅自改动！！" Margin ="10"  Grid.Row="1" Grid.Column ="0" Style="{StaticResource Label2}"/>

    </Grid>
</Window>
