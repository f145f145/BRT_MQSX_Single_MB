                            <Label Content="零点修正值" Grid.Row ="0" Grid.Column="9" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
                            <TextBox Text="{Binding Dev.AIList[0].ZeroCalValue,Mode =TwoWay}" Grid.Row ="0" Grid.Column="10" Style="{StaticResource TextBox1}" ToolTip ="修正后值=斜率*（标定后值+零点修正值）"/>
                            <Label Content="斜率修正值" Grid.Row ="1" Grid.Column="9" Style="{StaticResource Label1}" HorizontalAlignment="Center"/>
