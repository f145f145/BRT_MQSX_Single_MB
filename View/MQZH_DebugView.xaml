<Window x:Class="MQDFJ_MB.View.MQZH_DebugView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MQDFJ_MB.View"
        xmlns:converterPath="clr-namespace:MQDFJ_MB.UICtl.Converter"
        xmlns:popup="clr-namespace:MQDFJ_MB.View"
        mc:Ignorable="d"
        Title="幕墙四性装置手动调试" 
        Width="1600" Height="900" 
        WindowStartupLocation="CenterScreen" Background="White" ShowInTaskbar="True"
        Closing="Window_Closing" WindowState="Normal"
        DataContext="{Binding Source={StaticResource Locator},Path=Main_ViewModel}">

    <Window.Resources>
        <!--Slider模板-->
        <LinearGradientBrush x:Key="SliderBackground"  StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="#59ccfc"/>
            <GradientStop Offset="0.5" Color="#00b3fe"/>
            <GradientStop Offset="1" Color="#59ccfc"/>
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="SliderThumb"  StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="#FFD9D3E8"/>
            <!--<GradientStop Offset="1" Color="#dfdfdf"/>-->
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="SliderText"  StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="#7cce45"/>
            <GradientStop Offset="1" Color="#4ea017"/>
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="SliderBackground2"  StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="Gray"/>
            <GradientStop Offset="0.5" Color="Gray"/>
            <GradientStop Offset="1" Color="#59ccfc"/>
        </LinearGradientBrush>
        <Style x:Key="Slider_RepeatButton" TargetType="RepeatButton">
            <Setter Property="Focusable" Value="false" />
            <Setter Property="Height" Value="10"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border Background="{StaticResource SliderBackground}" />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="Slider_RepeatButton1" TargetType="RepeatButton">
            <Setter Property="Focusable" Value="false" />
            <Setter Property="Height" Value="10"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border Background="Gray" />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="Slider_Thumb" TargetType="Thumb">
            <Setter Property="Focusable" Value="false" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Thumb">
                        <Grid MinHeight="20">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Border Background="{StaticResource SliderBackground}"/>
                            <Border Grid.ColumnSpan="2" CornerRadius="4"  Background="{StaticResource SliderThumb}" Width="18">
                                <!--<TextBlock Text="||" HorizontalAlignment="Center" VerticalAlignment="Center"/>-->
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="Slider_CustomStyle" TargetType="Slider">
            <Setter Property="Focusable" Value="false" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Slider">
                        <Grid>
                            <!--<Grid.ColumnDefinitions>  
                                <ColumnDefinition Width="80"/>  
                                <ColumnDefinition/>  
                                <ColumnDefinition Width="40"/>  
                            </Grid.ColumnDefinitions>-->
                            <Grid.Effect>
                                <DropShadowEffect BlurRadius="8" ShadowDepth="1" />
                            </Grid.Effect>
                            <!--<Border HorizontalAlignment="Right" BorderBrush="Gray" BorderThickness="1,1,0,1" Background="{StaticResource SliderText}" Width="80" CornerRadius="8,0,0,8"/>
                            <Border Grid.Column="2" HorizontalAlignment="Right" BorderBrush="Gray" BorderThickness="0,1,1,1" Background="{StaticResource SliderText}" Width="40" CornerRadius="0,8,8,0"/>
                            <TextBlock Grid.Column="2" Text="{Binding RelativeSource={RelativeSource TemplatedParent},Path=Tag}" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14"/>
                            <TextBlock Grid.Column="0" Text="{Binding ElementName=PART_Track,Path=Value,StringFormat=\{0:N0\}}" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" DataContext="{Binding}" />-->

                            <Border Grid.Column="1" BorderBrush="Red" BorderThickness="0" CornerRadius="0,0,0,0">
                                <Track Grid.Column="1" Name="PART_Track">
                                    <Track.DecreaseRepeatButton>
                                        <RepeatButton Style="{StaticResource Slider_RepeatButton}"  
                                Command="Slider.DecreaseLarge"/>
                                    </Track.DecreaseRepeatButton>
                                    <Track.IncreaseRepeatButton>
                                        <RepeatButton Style="{StaticResource Slider_RepeatButton1}"  
                                Command="Slider.IncreaseLarge"/>
                                    </Track.IncreaseRepeatButton>
                                    <Track.Thumb>
                                        <Thumb Style="{StaticResource Slider_Thumb}"/>
                                    </Track.Thumb>
                                </Track>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--默认CheckBox样式-->
        <Style TargetType="CheckBox">
            <Style.Setters>
                <Setter Property="HorizontalAlignment" Value="Left"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontSize" Value="20"/>
                <Setter Property="FontWeight" Value="Bold"/>
                <Setter Property="Tag">
                    <Setter.Value>
                        <SolidColorBrush Color="Black"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="CheckBox">
                            <StackPanel Orientation="Horizontal">
                                <Border x:Name="bd" BorderBrush="Black" 
                                        BorderThickness="2" Margin="5,2">
                                    <Path x:Name="cp" Width="15" Height="15"  
                                          Stroke="Blue"  
                                          StrokeThickness="3"/>
                                </Border>
                                <ContentPresenter/>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsChecked" Value="True">
                                    <Setter TargetName="cp" Property="Data"  
                                            Value="M 0,6 L 6,12 12,0"/>
                                    <Setter TargetName="bd" 
                                            Property="BorderBrush">
                                        <Setter.Value>
                                            <SolidColorBrush Color="Blue"/>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style.Setters>
        </Style>
        <!--紧急按钮模板-->
        <ControlTemplate TargetType="Button" x:Key="EmStpButton">
            <Border Background="Yellow" Name="back" BorderBrush="Red" BorderThickness="1"
                    CornerRadius="3" Padding="5"
                    TextBlock.Foreground="Red" TextBlock.FontSize="18" TextBlock.FontWeight="Bold"
                    Width="120" Height ="50">
                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="back"  Property="Background" Value="Aqua"/>
                    <Setter Property="TextElement.Foreground" TargetName="back" Value="Red"/>
                </Trigger>
                <Trigger Property="IsPressed" Value="true">
                    <Setter TargetName="back" Property="Background" Value="BlueViolet"/>
                    <Setter Property="TextElement.Foreground" TargetName="back" Value="White"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <!--普通按钮模板-->
        <ControlTemplate TargetType="Button" x:Key="Button1">
            <Border Background="LightGray" Name="back" BorderBrush="Black" BorderThickness="0.8"
                    CornerRadius="3" Padding="5"
                    TextBlock.Foreground="Black" TextBlock.FontSize="18" Width="150" Height ="50">
                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="back"  Property="Background" Value="snow"/>
                </Trigger>
                <Trigger Property="IsPressed" Value="true">
                    <Setter TargetName="back" Property="Background" Value="CadetBlue"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>

        <!--Label模板1，参数名-->
        <Style x:Key="Label1" TargetType="Label">
            <Setter Property="FontSize" Value="18" />
            <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
            <Setter Property="Background" Value="White"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板2，参数值-->
        <Style x:Key="Label2" TargetType="Label">
            <Setter Property="Height" Value="40"/>
            <Setter Property="Width" Value="95"/>
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="FontFamily" Value="{DynamicResource DigifaceWide}"/>
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板3，参数单位-->
        <Style x:Key="Label3" TargetType="Label">
            <Setter Property="FontSize" Value="18"/>
            <Setter Property="Background" Value="White"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <!--Label模板4，状态标题-->
        <Style x:Key="Label4" TargetType="Label">
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5,5,5,5"/>
        </Style>
        <!--Label模板5，输出状态文字-->
        <Style x:Key="Label5" TargetType="Label">
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="FontFamily" Value="{StaticResource HeiTi}"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5,5,5,5"/>
        </Style>
        <!--圆圈模版1，绿色-->
        <Style x:Key="Ellipse1" TargetType="Ellipse">
            <Setter Property="Height" Value="20"/>
            <Setter Property="Width" Value="20"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5"/>
            <Setter Property="Fill" Value="Chartreuse"/>
        </Style>
        <!--圆圈模版2，无色-->
        <Style x:Key="Ellipse2" TargetType="Ellipse">
            <Setter Property="Height" Value="20"/>
            <Setter Property="Width" Value="20"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5"/>
            <Setter Property="Stroke" Value="Black"/>
            <Setter Property="Fill" Value="DarkGray"/>
        </Style>
        <!--TextBox模板2-->
        <Style x:Key="TextBox1" TargetType="{x:Type TextBox}">
            <Setter Property="Height" Value="50"/>
            <Setter Property="Width" Value="120"/>
            <Setter Property="Margin" Value="5,5,5,0"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="FontSize" Value="15"/>
            <Setter Property="TextWrapping" Value="Wrap"/>
            <Setter Property="Background" Value="GhostWhite"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="KeyboardNavigation.TabNavigation" Value="None"/>
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="AllowDrop" Value="true"/>
            <Setter Property="ScrollViewer.PanningMode" Value="VerticalFirst"/>
            <Setter Property="Stylus.IsFlicksEnabled" Value="False"/>
        </Style>
        <!--DataGrid-->
        <Style x:Key="DataGridStyle" TargetType="DataGrid">
            <Setter Property="ColumnHeaderStyle" Value="{DynamicResource ColumnHeaderStyle}"></Setter>
            <Setter Property="CellStyle" Value="{DynamicResource CellStyle}"></Setter>
            <Setter Property="RowStyle" Value="{DynamicResource RowStyle}"></Setter>
            <Setter Property="Background" Value="White"></Setter>
            <Setter Property="EnableRowVirtualization" Value="False"></Setter>
            <Setter Property="GridLinesVisibility" Value="None"></Setter>
            <Setter Property="CanUserAddRows" Value="False"></Setter>
            <!--禁止在尾行手动添加数据-->
            <Setter Property="AutoGenerateColumns" Value="False"></Setter>
            <Setter Property="IsEnabled" Value="True"></Setter>
        </Style>
        <Style x:Key="ColumnHeaderStyle" TargetType="DataGridColumnHeader">
            <Setter Property="Height" Value="35"></Setter>
            <Setter Property="Background" Value="#F2F2F2"></Setter>
            <Setter Property="BorderThickness" Value="1"></Setter>
            <Setter Property="BorderBrush" Value="#CBCBCB"></Setter>
            <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
            <!--水平居中-->
            <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
            <!--垂直居中-->
        </Style>
        <Style x:Key="RowStyle" TargetType="DataGridRow">
            <Setter Property="Cursor" Value="Hand"></Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter Property="Background" Value="#F2F2F2"/>
                </Trigger>
                <Trigger Property="IsSelected" Value="True">
                    <Setter Property="Background"  Value="#CBCBCB" />
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="CellStyle" TargetType="DataGridCell">
            <Setter Property="Height" Value="35"></Setter>
            <Setter Property="FontSize" Value="13"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="DataGridCell">
                        <Border x:Name="Bg" Background="Transparent" BorderThickness="1" UseLayoutRounding="True" BorderBrush="#FFCBCBCB">
                            <ContentPresenter HorizontalAlignment="Center"  VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>

                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsSelected" Value="True">
                    <Setter Property="Background"  Value="#CBCBCB" />
                    <Setter Property="Foreground" Value="#000000" />
                </Trigger>
            </Style.Triggers>
        </Style>
        <!--值转换器-->
        <converterPath:BoolToVisibilityConverter x:Key="BVConverter"/>
        <converterPath:BoolToVisibilityNotConverter x:Key="BVNotConverter"/>
        <converterPath:BoolToCollorGConverter x:Key="BGConverter"/>
        <converterPath:BoolToCollorOConverter x:Key="BOConverter"/>
        <converterPath:BoolToCollorNotOConverter x:Key="BNotOConverter"/>
        <converterPath:BoolToCollorRConverter x:Key="BRConverter"/>
        <converterPath:IntToCollorOConverter x:Key="ComCollorConverter"/>
        <converterPath:StrToCollorOConverter x:Key="SGBOConverter"/>

        <converterPath:IntToFFModeConverter x:Key="FFModeConverter"/>
        <converterPath:IntToFFErrConverter x:Key="FFErrConverter"/>
        <converterPath:IntToFFSTConverter x:Key="FFSTConverter"/>
        <converterPath:IntToFFSDConverter x:Key="FFSDConverter"/>
        <converterPath:IntToFFZDZBConverter x:Key="FFZDZBConverter"/>
        <converterPath:IntToFFFDZBConverter x:Key="FFFDZBConverter"/>
        <converterPath:IntToFFZBConverter x:Key="FFZBConverter"/>
        <converterPath:IntToFFFBConverter x:Key="FFFBConverter"/>
        <converterPath:IntToFFActionConverter x:Key="FFActionConverter"/>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="180" MinHeight ="180" MaxHeight ="600"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!--======================================菜单栏===================================================-->
        <Menu x:Name="DebugMenu" Grid.Row="0" VerticalAlignment="Center" Grid.ColumnSpan="2">

            <MenuItem VerticalAlignment="Stretch" Header="调零标定"  FontSize="16" Height ="40">
                <MenuItem Header="传感器调零" VerticalAlignment="Stretch" Click ="Menu_Corr_Click" FontSize="16" Height ="40"/>
            </MenuItem>

            <MenuItem Header="退出调试"  VerticalAlignment="Stretch" FontSize="16" Height ="40">
                <MenuItem Header="退出调试" VerticalAlignment="Stretch" Click ="Menu_Quit_Click" FontSize="16" Height ="40"/>
            </MenuItem>
        </Menu>

        <!--======================================上半屏，参数数据显示区块===================================================-->
        <Border Grid.Row="1" BorderBrush="Black" BorderThickness="1" CornerRadius="5"  HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="10" Padding="5" Grid.ColumnSpan="2">
            <Grid >
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>

                <Label Content="压差小：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="0"/>
                <Label Content="压差中：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="3" Visibility ="{Binding Dev.IsWithCYM, Converter={StaticResource BVConverter}}"/>
                <Label Content="压差大：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="6"/>
                <Label Content="{Binding Dev.AIList[12].ValueFinal}" ContentStringFormat="{}{0:0 }" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="1"/>
                <Label Content="{Binding Dev.AIList[13].ValueFinal}" ContentStringFormat="{}{0:0 }" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="4" Visibility ="{Binding Dev.IsWithCYM, Converter={StaticResource BVConverter}}"/>
                <Label Content="{Binding Dev.AIList[14].ValueFinal}" ContentStringFormat="{}{0:0 }" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="7"/>
                <Label Content="Pa" Style="{StaticResource Label3}" Grid.Row="0" Grid.Column ="2"/>
                <Label Content="Pa" Style="{StaticResource Label3}" Grid.Row="0" Grid.Column ="5" Visibility ="{Binding Dev.IsWithCYM, Converter={StaticResource BVConverter}}"/>
                <Label Content="Pa" Style="{StaticResource Label3}" Grid.Row="0" Grid.Column ="8"/>

                <Label Content="风量1：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="0" Visibility ="{Binding Dev.WithCSFG1, Converter={StaticResource BVConverter}}"/>
                <Label Content="风量2：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="3" Visibility ="{Binding Dev.WithCSFG2, Converter={StaticResource BVConverter}}"/>
                <Label Content="风量3：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="6" Visibility ="{Binding Dev.WithCSFG3, Converter={StaticResource BVConverter}}"/>
                <Label Content="{Binding Dev.FL1}" ContentStringFormat="{}{0:0.00}" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="1" Visibility ="{Binding Dev.WithCSFG1, Converter={StaticResource BVConverter}}"/>
                <Label Content="{Binding Dev.FL2}" ContentStringFormat="{}{0:0.00}" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="4" Visibility ="{Binding Dev.WithCSFG2, Converter={StaticResource BVConverter}}"/>
                <Label Content="{Binding Dev.FL3}" ContentStringFormat="{}{0:0.00}" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="7" Visibility ="{Binding Dev.WithCSFG3, Converter={StaticResource BVConverter}}"/>
                <Label Content="m³/h" Style="{StaticResource Label3}" Grid.Row="1" Grid.Column ="2" Visibility ="{Binding Dev.WithCSFG1, Converter={StaticResource BVConverter}}"/>
                <Label Content="m³/h" Style="{StaticResource Label3}" Grid.Row="1" Grid.Column ="5" Visibility ="{Binding Dev.WithCSFG2, Converter={StaticResource BVConverter}}"/>
                <Label Content="m³/h" Style="{StaticResource Label3}" Grid.Row="1" Grid.Column ="8" Visibility ="{Binding Dev.WithCSFG3, Converter={StaticResource BVConverter}}"/>

                <Label Content="风频率：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="0"/>
                <Label Content="水频率：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="3" Visibility="{Binding Dev.IsWithSLL ,Converter={StaticResource BVConverter}}"/>
                <Label Content="水流量：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="6" Visibility="{Binding Dev.IsWithSLL ,Converter={StaticResource BVConverter}}"/>
                <Label Content="{Binding Dev.AIList[22].ValueFinal}" ContentStringFormat="{}{0:0.0 }" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="1"/>
                <Label Content="{Binding Dev.AIList[23].ValueFinal}" ContentStringFormat="{}{0:0.0 }" Visibility="{Binding Dev.IsWithSLL ,Converter={StaticResource BVConverter}}" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="4"/>
                <Label Content="{Binding Dev.SLL}" ContentStringFormat="{}{0:0.00}" Visibility="{Binding Dev.IsWithSLL ,Converter={StaticResource BVConverter}}" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="7"/>
                <Label Content="Hz" Style="{StaticResource Label3}" Grid.Row="2" Grid.Column ="2"/>
                <Label Content="Hz" Style="{StaticResource Label3}" Grid.Row="2" Grid.Column ="5" Visibility="{Binding Dev.IsWithSLL ,Converter={StaticResource BVConverter}}"/>
                <Label Content="m³/h" Style="{StaticResource Label3}" Grid.Row="2" Grid.Column ="8" Visibility="{Binding Dev.IsWithSLL ,Converter={StaticResource BVConverter}}"/>

                <Label Content="位移1：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="9"/>
                <Label Content="位移2：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="9"/>
                <Label Content="位移3：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="9"/>
                <Label Content="位移4：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="12"/>
                <Label Content="位移5：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="12"/>
                <Label Content="位移6：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="12"/>
                <Label Content="位移7：" Style="{StaticResource Label1}" Grid.Row="0" Grid.Column ="15"/>
                <Label Content="位移8：" Style="{StaticResource Label1}" Grid.Row="1" Grid.Column ="15"/>
                <Label Content="位移9：" Style="{StaticResource Label1}" Grid.Row="2" Grid.Column ="15"/>
                <Label Content="位移10：" Style="{StaticResource Label1}" Visibility="{Binding Dev.Is12WY ,Converter={StaticResource BVConverter}}" Grid.Row="0" Grid.Column ="18"/>
                <Label Content="位移11：" Style="{StaticResource Label1}" Visibility="{Binding Dev.Is12WY ,Converter={StaticResource BVConverter}}" Grid.Row="1" Grid.Column ="18"/>
                <Label Content="位移12：" Style="{StaticResource Label1}" Visibility="{Binding Dev.Is12WY ,Converter={StaticResource BVConverter}}" Grid.Row="2" Grid.Column ="18"/>
                <Label Content="{Binding Dev.AIList[0].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="10"/>
                <Label Content="{Binding Dev.AIList[1].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="10"/>
                <Label Content="{Binding Dev.AIList[2].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="10"/>
                <Label Content="{Binding Dev.AIList[3].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="13"/>
                <Label Content="{Binding Dev.AIList[4].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="13"/>
                <Label Content="{Binding Dev.AIList[5].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="13"/>
                <Label Content="{Binding Dev.AIList[6].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="16"/>
                <Label Content="{Binding Dev.AIList[7].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="1" Grid.Column ="16"/>
                <Label Content="{Binding Dev.AIList[8].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Style="{StaticResource Label2}" Grid.Row="2" Grid.Column ="16"/>
                <Label Content="{Binding Dev.AIList[9].ValueFinal}" ContentStringFormat="{}{0:0.00 }" Visibility="{Binding Dev.Is12WY ,Converter={StaticResource BVConverter}}" Style="{StaticResource Label2}" Grid.Row="0" Grid.Column ="19"/>
