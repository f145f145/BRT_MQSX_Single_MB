<Window x:Class="MQDFJ_MB.View.DTFY.MQZH_DTFYView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MQDFJ_MB.View.DTFY"
        xmlns:converterPath="clr-namespace:MQDFJ_MB.UICtl.Converter"
        mc:Ignorable="d"
        Title="动态风压下水密性能检测" 
        Width="1920" Height="1000" 
        WindowStartupLocation="CenterScreen" Background="White" ShowInTaskbar="True"
        Closing="Window_Closing" WindowState="Maximized"
        DataContext="{Binding Source={StaticResource Locator},Path=Main_ViewModel}">

    <TabControl HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="10" Grid.Row="1"  >
        <TabItem Header="【试验操作】" Background="#00538A">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="100"/>
                    <RowDefinition Height="400"/>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1000"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!--======================================压力点参数、数据===================================================-->
                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderBrush="Black" BorderThickness="1" CornerRadius="5"  HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="5" Background="White">
                    <Grid Background ="WhiteSmoke">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="50"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="50"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="50"/>
                            <ColumnDefinition Width="100"/>
                            <ColumnDefinition Width="100"/>
                            <ColumnDefinition Width="100"/>
                        </Grid.ColumnDefinitions>

                        <Label Content="试验编号：" Grid.Row="0" Grid.Column ="0" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.TestNO, Mode =OneWay}" Grid.Row="0" Grid.Column ="1" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="测试时间：" Grid.Row="1" Grid.Column ="0" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.TestTime, Mode =OneWay}" ContentStringFormat="yyyy-MM-dd HH:mm:ss" Grid.Row="1" Grid.Column ="1" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <Label Content="是否完成：" Grid.Row="0" Grid.Column ="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.CommpletedStr, Mode =OneWay}" Grid.Row="0" Grid.Column ="3" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="是否可用：" Grid.Row="0" Grid.Column ="4" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.UsefullStr, Mode =OneWay}" Grid.Row="0" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="是否在用：" Grid.Row="0" Grid.Column ="6" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.UsingStr, Mode =OneWay}" Grid.Row="0" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <Label Content="测试距离：" Grid.Row="1" Grid.Column ="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <TextBox Grid.Row ="1" Grid.Column="3" Grid.ColumnSpan="2" Text="{Binding PublicData.SM_DTFY_CaliDQ.DistanceSet, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="30" Width ="120" FontSize="18"/>
                        <Label Content="mm" Grid.Row="1" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        
                    </Grid>
                </Border>

                <!--======================================压力点参数、数据===================================================-->
                <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" BorderBrush="Black" BorderThickness="1" CornerRadius="5"  HorizontalAlignment="stretch" VerticalAlignment="stretch" Margin="5" Background="White">
                    <Grid Background ="WhiteSmoke">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="80"/>
                        </Grid.ColumnDefinitions>

                        <!--变频器1调速-->
                        <Label Content="压力点序号" Grid.Row="0" Grid.Column ="0" Grid.RowSpan="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="设定压力值" Grid.Row="0" Grid.Column ="1" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="设定风速值" Grid.Row="0" Grid.Column ="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="测试起始频率" Grid.Row="0" Grid.Column ="3" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="频率调整步长" Grid.Row="0" Grid.Column ="4" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="是否完成" Grid.Row="0" Grid.Column ="5" Grid.RowSpan="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="测试开始时间" Grid.Row="0" Grid.Column ="6" Grid.RowSpan="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="左上风速值" Grid.Row="0" Grid.Column ="7" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="右上风速值" Grid.Row="0" Grid.Column ="8" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="左下风速值" Grid.Row="0" Grid.Column ="9" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="右上风速值" Grid.Row="0" Grid.Column ="10" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="频率测定值" Grid.Row="0" Grid.Column ="11" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="是否合格" Grid.Row="0" Grid.Column ="12" Grid.RowSpan="2" Margin="0" FontWeight="ExtraBold" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <Label Content="Pa" Grid.Row="1" Grid.Column ="1" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="m/s" Grid.Row="1" Grid.Column ="2" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="Hz" Grid.Row="1" Grid.Column ="3" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="Hz" Grid.Row="1" Grid.Column ="4" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="m/s" Grid.Row="1" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="m/s" Grid.Row="1" Grid.Column ="8" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="m/s" Grid.Row="1" Grid.Column ="9" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="m/s" Grid.Row="1" Grid.Column ="10" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="Hz" Grid.Row="1" Grid.Column ="11" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <!--压力点序号-->
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].PointNo, Mode =OneWay, StringFormat={}{0:F1}}" Grid.Row="2" Grid.Column ="0" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].PointNo, Mode =OneWay, StringFormat={}{0:F1}}" Grid.Row="3" Grid.Column ="0" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].PointNo, Mode =OneWay, StringFormat={}{0:F1}}" Grid.Row="4" Grid.Column ="0" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].PointNo, Mode =OneWay, StringFormat={}{0:F1}}" Grid.Row="5" Grid.Column ="0" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].PointNo, Mode =OneWay, StringFormat={}{0:F1}}" Grid.Row="6" Grid.Column ="0" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                      
                        <!--设定压力-->
                        <TextBox Grid.Row ="2" Grid.Column="1" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].PressSet, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="3" Grid.Column="1" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].PressSet, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="4" Grid.Column="1" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].PressSet, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="5" Grid.Column="1" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].PressSet, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="6" Grid.Column="1" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].PressSet, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].SpeedSet, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="2" Grid.Column ="2" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].SpeedSet, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="3" Grid.Column ="2" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].SpeedSet, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="4" Grid.Column ="2" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].SpeedSet, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="5" Grid.Column ="2" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].SpeedSet, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="6" Grid.Column ="2" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <!--起始测试频率-->
                        <TextBox Grid.Row ="2" Grid.Column="3" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].FrequencyStart, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="3" Grid.Column="3" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].FrequencyStart, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="4" Grid.Column="3" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].FrequencyStart, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="5" Grid.Column="3" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].FrequencyStart, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="6" Grid.Column="3" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].FrequencyStart, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>

                        <!--频率调整步长-->
                        <TextBox Grid.Row ="2" Grid.Column="4" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].FrequencyChangeStep, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="3" Grid.Column="4" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].FrequencyChangeStep, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="4" Grid.Column="4" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].FrequencyChangeStep, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="5" Grid.Column="4" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].FrequencyChangeStep, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>
                        <TextBox Grid.Row ="6" Grid.Column="4" Text="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].FrequencyChangeStep, Mode =TwoWay, StringFormat={}{0:F2}}" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height ="40" Width ="100" FontSize="18"/>

                        <!--开始测试时间-->
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].TestTimeStart, Mode =OneWay}" ContentStringFormat="yyyy-MM-dd HH:mm:ss"  Grid.Row="2" Grid.Column ="6" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].TestTimeStart, Mode =OneWay}" ContentStringFormat="yyyy-MM-dd HH:mm:ss"  Grid.Row="3" Grid.Column ="6" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].TestTimeStart, Mode =OneWay}" ContentStringFormat="yyyy-MM-dd HH:mm:ss"  Grid.Row="4" Grid.Column ="6" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].TestTimeStart, Mode =OneWay}" ContentStringFormat="yyyy-MM-dd HH:mm:ss"  Grid.Row="5" Grid.Column ="6" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].TestTimeStart, Mode =OneWay}" ContentStringFormat="yyyy-MM-dd HH:mm:ss"  Grid.Row="6" Grid.Column ="6" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <!--完成情况-->
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].CommpletedStr, Mode =OneWay}" Grid.Row="2" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].CommpletedStr, Mode =OneWay}" Grid.Row="3" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].CommpletedStr, Mode =OneWay}" Grid.Row="4" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].CommpletedStr, Mode =OneWay}" Grid.Row="5" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].CommpletedStr, Mode =OneWay}" Grid.Row="6" Grid.Column ="5" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <!--左上风速测定值-->
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].SpeedMaxUL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="2" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].SpeedMaxUL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="3" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].SpeedMaxUL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="4" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].SpeedMaxUL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="5" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].SpeedMaxUL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="6" Grid.Column ="7" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <!--右上风速测定值-->
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].SpeedMaxUR, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="2" Grid.Column ="8" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].SpeedMaxUR, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="3" Grid.Column ="8" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].SpeedMaxUR, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="4" Grid.Column ="8" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].SpeedMaxUR, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="5" Grid.Column ="8" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].SpeedMaxUR, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="6" Grid.Column ="8" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <!--左下风速测定值-->
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[0].SpeedMaxDL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="2" Grid.Column ="9" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[1].SpeedMaxDL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="3" Grid.Column ="9" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[2].SpeedMaxDL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="4" Grid.Column ="9" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[3].SpeedMaxDL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="5" Grid.Column ="9" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>
                        <Label Content="{Binding PublicData.SM_DTFY_CaliDQ.PointList[4].SpeedMaxDL, Mode =OneWay}" ContentStringFormat="{}{0:F2}"  Grid.Row="6" Grid.Column ="9" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18"/>

                        <!--右下风速测定值-->
